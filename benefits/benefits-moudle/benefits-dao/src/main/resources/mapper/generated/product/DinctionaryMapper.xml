<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.benefits.mapper.product.DictionaryMapper">
    <resultMap type="com.lx.benefits.bean.entity.product.DictionaryEntity" id="DictionaryResult">
        <result property="id" column="id" />
        <result property="name" column="name" />
        <result property="nameEn" column="name_en" />
        <result property="sort" column="sort" />
        <result property="status" column="status" />
        <result property="type" column="type" />
        <result property="createdUser" column="created_user" />
        <result property="createdTime" column="created_time" />
        <result property="updateUser" column="update_user" />
        <result property="updatedTime" column="updated_time" />
    </resultMap>
	
	<sql id="selectDictionaryVo">
        select id, name, name_en, sort, status, created_user, created_time, update_user, updated_time,type from dictionary
    </sql>

    <sql id="Example_Where_Clause">
        <where>
            <if test="id != null "> and id = #{id}</if>
            <if test="name != null  and name != '' "> and name = #{name}</if>
            <if test="nameEn != null  and nameEn != '' "> and name_en = #{nameEn}</if>
            <if test="sort != null "> and sort = #{sort}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="createdUser != null  and createdUser != '' "> and created_user = #{createdUser}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updateUser != null  and updateUser != '' "> and update_user = #{updateUser}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </sql>
	
    <select id="selectList" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity" resultMap="DictionaryResult">
        <include refid="selectDictionaryVo"/>
        <include refid="Example_Where_Clause"/>
    </select>
    
    <select id="selectById" parameterType="Long" resultMap="DictionaryResult">
        <include refid="selectDictionaryVo"/>
        where id = #{id}
    </select>
        
    <insert id="insert" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity" useGeneratedKeys="true" keyProperty="id">
        insert into dictionary
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="name != null  and name != ''  ">name,</if>
			<if test="nameEn != null  and nameEn != ''  ">name_en,</if>
			<if test="sort != null  ">sort,</if>
			<if test="status != null  ">status,</if>
            <if test="type != null  ">type,</if>
			<if test="createdUser != null  and createdUser != ''  ">created_user,</if>
			<if test="createdTime != null  ">created_time,</if>
			<if test="updateUser != null  and updateUser != ''  ">update_user,</if>
			<if test="updatedTime != null  ">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="name != null  and name != ''  ">#{name},</if>
			<if test="nameEn != null  and nameEn != ''  ">#{nameEn},</if>
			<if test="sort != null  ">#{sort},</if>
			<if test="status != null  ">#{status},</if>
            <if test="type != null  ">#{type},</if>
			<if test="createdUser != null  and createdUser != ''  ">#{createdUser},</if>
			<if test="createdTime != null  ">#{createdTime},</if>
			<if test="updateUser != null  and updateUser != ''  ">#{updateUser},</if>
			<if test="updatedTime != null  ">#{updatedTime},</if>
         </trim>
    </insert>
	 
    <update id="update" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity">
        update dictionary
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null  and name != ''  ">name = #{name},</if>
            <if test="nameEn != null  and nameEn != ''  ">name_en = #{nameEn},</if>
            <if test="sort != null  ">sort = #{sort},</if>
            <if test="status != null  ">status = #{status},</if>
            <if test="type != null  ">type = #{type},</if>
            <if test="createdUser != null  and createdUser != ''  ">created_user = #{createdUser},</if>
            <if test="createdTime != null  ">created_time = #{createdTime},</if>
            <if test="updateUser != null  and updateUser != ''  ">update_user = #{updateUser},</if>
            <if test="updatedTime != null  ">updated_time = #{updatedTime},</if>
        </trim>
        where id = #{id}
    </update>

	<delete id="deleteById" parameterType="Long">
        delete from dictionary where id = #{id}
    </delete>
	
    <delete id="deleteByIds" parameterType="String">
        delete from dictionary where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="queryByParam" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity" resultMap="DictionaryResult">
        <include refid="selectDictionaryVo"/>
        <include refid="Example_Where_Clause" />
    </select>

    <select id="selectCount" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity" resultType="java.lang.Integer">
        select count(*) from dictionary
        <include refid="Example_Where_Clause" />
    </select>

    <select id="selectPageList" parameterType="com.lx.benefits.bean.entity.product.DictionaryEntity" resultMap="DictionaryResult">
        <include refid="selectDictionaryVo"/>
        <include refid="Example_Where_Clause" />
        <if test="orderByClause != null">
            order by ${orderByClause}
        </if>
        <if test="start != null and limit != null">
            LIMIT #{start}, #{limit}
        </if>
    </select>
    
</mapper>