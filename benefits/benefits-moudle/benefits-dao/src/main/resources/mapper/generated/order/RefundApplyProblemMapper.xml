	</delete>
	
	<update id="updateByPrimaryKeySelective">
		UPDATE refund_apply_problem
		<set>
		<if test="id!=null" >`ID`=#{id,jdbcType=INTEGER},</if>
		<if test="name!=null" >`NAME`=#{name,jdbcType=VARCHAR},</if>
		<if test="type!=null" >`TYPE`=#{type,jdbcType=TINYINT},</if>
