	<resultMap id="BaseResultMap" type="com.lx.benefits.bean.entity.order.Order">
		<id property="id" column="ID" /><!--主键ID-->
		<result property="number" column="NUMBER" /><!--订单编号-->
		<result property="parentNumber" column="PARENT_NUMBER" /><!--父订单ID，商品级订单的父订单ID为店铺级订单ID，店铺级订单的父订单ID为支付级订单ID，支付级订单无父级订单(值为0)-->
		<result property="level" column="LEVEL" /><!--订单级别，1:支付级订单(L1),2:商家级订单(L2),3:商品级订单(L3)-->
		<result property="type" column="TYPE" /><!--订单类型，1:默认商品购买-->
		<result property="marketplace" column="MARKETPLACE" /><!--交易市场，1:福粒-->
		<result property="platform" column="PLATFORM" /><!--下单来源平台，0:全终端(未知平台下单),1:IOS下单,2:Android下单,3:H5下单
-->
		<result property="visible" column="VISIBLE" /><!--订单可见状态，0:订单正常可见,1:订单初始删除,2:订单永久删除,99:订单初始不可见-->
		<result property="status" column="STATUS" /><!--订单主状态，0:订单已创建,1:订单已取消,2:订单已支付,3:订单已发货,4:订单已收货,5:订单已关闭,6:订单已完成-->
		<result property="statusEx" column="STATUS_EX" /><!--订单子状态，0:订单已创建,1:订单已取消,2:订单已支付,3:订单已发货,4:订单已收货,5:订单已关闭,6:订单已完成-->
		<result property="reverseStatus" column="REVERSE_STATUS" /><!--订单逆向状态，0:未进入退货/款流程,1:进入退货/款流程,2:退货/款流程完成（仅退款行为）,3:退货/款流程完结，未生过成功的售后行为,4:退货/款流程完结，发生过成功的售后行为-->
		<result property="rateStatus" column="RATE_STATUS" /><!--订单评价状态，0:初始不可评价,1:初始可评价,2:可追加评价,3:追加评价后,评价关闭-->
		<result property="price" column="PRICE" /><!--订单货款（不包含积分），商品级订单表示购买数量*商品原价-商品级优惠，商家级订单表示其子商品级订单货款总和减去商家优惠，支付级订单表示其子商家级订单货款总和减去平台优惠-->
		<result property="pointAmount" column="POINT_AMOUNT" /><!--订单积分抵扣货款-->
		<result property="cardAmount" column="CARD_AMOUNT" /><!--订单会员卡积分抵扣货款-->
		<result property="quantity" column="QUANTITY" /><!--数量，商品级订单表示购买SKU数量，商家级订单表示子商品级订单购买SKU数量总和，支付级订单表示子商家级订单数量总和-->
		<result property="buyerUserId" column="BUYER_USER_ID" /><!--买家用户ID-->
		<result property="sellerId" column="SELLER_ID" /><!--卖家ID-->
		<result property="spuCode" column="SPU_CODE" /><!--商品 编号，商品级订单专用-->
		<result property="skuId" column="SKU_ID" /><!--SKU ID，商品级订单专用-->
		<result property="payId" column="PAY_ID" /><!--支付ID-->
		<result property="payTime" column="PAY_TIME" /><!--支付时间-->
		<result property="expireTime" column="EXPIRE_TIME" /><!--当前状态到下一个状态的超时截止时间-->
		<result property="settleTime" column="SETTLE_TIME" /><!--结算时间-->
		<result property="shipExpense" column="SHIP_EXPENSE" /><!--商家运费(不包含积分)-->
		<result property="shipExpensePointAmount" column="SHIP_EXPENSE_POINT_AMOUNT" /><!--积分抵扣运费-->
		<result property="shipTime" column="SHIP_TIME" /><!--发货时间-->
		<result property="receiveTime" column="RECEIVE_TIME" /><!--收货时间-->
		<result property="receiveType" column="RECEIVE_TYPE" /><!--收货类型-->
		<result property="cancelTime" column="CANCEL_TIME" /><!--取消时间-->
		<result property="cancelReason" column="CANCEL_REASON" /><!--取消原因，JSON存储-->
		<result property="sellerComment" column="SELLER_COMMENT" /><!--卖家备注-->
		<result property="buyerComment" column="BUYER_COMMENT" /><!--买家备注-->
		<result property="campaignId" column="CAMPAIGN_ID" /><!--节日礼金ID-->
		<result property="promotionAmount" column="PROMOTION_AMOUNT" /><!--优惠金额，支付级订单表示平台优惠金额，商家级订单用于改价，商品级订单暂不启用-->
		<result property="promotion" column="PROMOTION" /><!--促销快照，JSON存储-->
		<result property="source" column="SOURCE" /><!--订单来源 0：购物车，1：立即购买，2：代下单，3：预售
-->
		<result property="thirdOrderNumber" column="THIRD_ORDER_NUMBER" /><!--第三方订单编号-->
		<result property="extra" column="EXTRA" /><!--订单扩展，JSON存储-->
		<result property="tags" column="TAGS" /><!--订单标签-->
		<result property="version" column="VERSION" /><!--版本号-->
		<result property="createTime" column="CREATE_TIME" /><!--创建时间，创建时间为下单时间-->
		<result property="updateTime" column="UPDATE_TIME" /><!--修改时间-->
		<result property="num" column="num" javaType="INTEGER" />
		<result property="voucherId" column="voucher_id" />
		<result property="seckillId" column="seckill_id" />
		<result property="voucherBenefit" column="voucher_benefit" />
	</resultMap>

